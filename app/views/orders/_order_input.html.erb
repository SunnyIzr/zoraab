<div data-sub="<%=sub_id%>">
  <% items.times do |item| %>
    <div class="left" data-item="<%=item%>" style="padding: 0 10px">
      <div class="preview-image text-center hide">
        <%= image_tag 'no-prev.jpg'%>
      </div>
      <div data-subid= <%=sub_id%> class="arrow-tray editable">
        <div class="text-center hide arrows left">
          <a class="prev-link" data-item="<%=item%>" data-subid= <%=sub_id%> href="#">
            <i class="fa fa-arrow-circle-o-left"></i>
          </a>
        </div>
        <div class="text-center hide arrows right">
          <a class="next-link" data-item="<%=item%>" data-subid= <%=sub_id%> href="#">
            <i class="fa fa-arrow-circle-o-right"></i>
          </a>
        </div>
      </div>
      <div><%= text_field_tag 'item[]', nil , class: 'selection editable' %></div>
    </div>
  <%end%>
</div>

